{
  "private": true,
  "name": "dsr-index",
  "version": "2.0.1",
  "description": "Index page of all DSR domains.",
  "author": "DSRKafuU <dsrkafuu@outlook.com> (https://dsrkafuu.su)",
  "license": "Apache-2.0",
  "scripts": {
    "hugo:dev": "hugo server",
    "hugo:prod": "hugo --cleanDestinationDir",
    "rollup": "rollup -c",
    "minify": "node ./scripts/minify.js",
    "data": "node ./scripts/data.js",
    "dev": "cross-env NODE_ENV=development env-cmd concurrently npm:hugo:dev",
    "build": "cross-env NODE_ENV=production env-cmd concurrently -m 1 npm:hugo:prod npm:minify npm:rollup"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "~7.14.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.2",
