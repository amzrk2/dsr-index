    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "autoprefixer": "^10.2.5",
    "concurrently": "^6.1.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.2",
    "env-cmd": "^10.1.0",
    "html-minifier-terser": "^5.1.1",
    "postcss": "^8.2.15",
    "prettier": "^2.3.0",
    "rollup-plugin-terser": "^7.0.2"
  }
